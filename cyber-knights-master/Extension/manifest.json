{
    "manifest_version": 2,

    "name" : "Phishing Website Detector", 
    "description": "Detects a Phishing Website",
    "version": "0.1",

    "browser_action": {
        "default_pop": "popup.html"
    },

    "background": {
        "scripts": ["background.js"]
    },

    "permissions": [
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}